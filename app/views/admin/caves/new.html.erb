<div class>
  <%= form_with model: @cafe, url:'/admin/caves', local:true do |f| %>

    <h4 class>name</h4>
    <%= f.text_field :name %>

    <h4 class>caption</h4>
    <%= f.text_area :caption %>

    <h4 class>phone_number</h4>
    <%= f.text_field :phone_number %>

    <h4 class>email</h4>
    <%= f.text_field :email %>

    <h4 class>url</h4>
    <%= f.text_field :url %>


    <%= f.submit 'Create Menu' %>
  <% end %>

</div>